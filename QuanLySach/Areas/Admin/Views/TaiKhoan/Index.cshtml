@model IEnumerable<QuanLySach.Models.TaiKhoan>

@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_Layout_Admin.cshtml";
}

<h2>Quản lý tài khoản</h2>

<p>
    @Html.ActionLink("Tạo tài khoản mới", "Create")
</p>
<table id="tbTaiKhoan" class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.First().TenDangNhap)
        </th>
        <th>
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.TenDangNhap)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.TenDangNhap }) |
                @Html.ActionLink("Details", "Details", new { id = item.TenDangNhap }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.TenDangNhap })
            </td>
        </tr>
    }

</table>